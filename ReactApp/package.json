{
	"name": "gutenbergreactapp",
	"private": true,
	"homepage": "./",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"dev:remote": "vite --host --config vite.config.remote.js",
		"build": "vite build && vite build --emptyOutDir=false --config vite.config.remote.js",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"format": "prettier --write .",
		"preview": "vite preview --host"
	},
	"dependencies": {
		"@monaco-editor/react": "^4.6.0",
		"@wordpress/api-fetch": "^7.7.0",
		"@wordpress/block-editor": "^14.2.0",
		"@wordpress/block-library": "^9.7.0",
		"@wordpress/core-data": "^7.7.0",
		"@wordpress/edit-post": "^8.7.0",
		"@wordpress/editor": "^14.7.0",
		"@wordpress/element": "^6.7.0",
		"@wordpress/format-library": "^5.7.0",
		"@wordpress/notices": "^5.9.0",
		"@wordpress/preferences": "^4.7.0",
		"@wordpress/private-apis": "^1.7.0",
		"react": "^18.3.1"
	},
	"devDependencies": {
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@vitejs/plugin-react": "^4.2.1",
		"@wordpress/dependency-extraction-webpack-plugin": "^6.6.0",
		"@wordpress/prettier-config": "^4.3.0",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"magic-string": "^0.30.11",
		"prettier": "^3.3.2",
		"vite": "^5.2.0"
	}
}
