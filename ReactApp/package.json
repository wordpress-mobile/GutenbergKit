{
	"name": "gutenbergreactapp",
	"private": true,
	"homepage": "./",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"dev:remote": "vite --host --config vite.config.remote-editor.js",
		"build": "vite build",
		"build:remote": "vite build --config vite.config.remote-editor.js",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"format": "prettier --write .",
		"preview": "vite preview --host",
		"preview:remote": "vite preview --host --config vite.config.remote-editor.js"
	},
	"dependencies": {
		"@monaco-editor/react": "^4.6.0",
		"@wordpress/api-fetch": "^7.4.0",
		"@wordpress/block-editor": "^14.0.0",
		"@wordpress/block-library": "^9.5.0",
		"@wordpress/core-data": "^7.5.0",
		"@wordpress/editor": "^14.5.0",
		"@wordpress/element": "^6.5.0",
		"@wordpress/format-library": "^5.5.0",
		"react": "^18.3.1"
	},
	"devDependencies": {
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@vitejs/plugin-react": "^4.2.1",
		"@wordpress/dependency-extraction-webpack-plugin": "^6.6.0",
		"@wordpress/prettier-config": "^4.3.0",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"prettier": "^3.3.2",
		"vite": "^5.2.0"
	}
}
